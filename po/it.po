#: src/parse_args.c:594
#, c-format
msgid ""
"%s - edit files as another user\n"
"\n"
msgstr ""
"%s - modifica file come un altro utente\n"
"\n"

#: src/parse_args.c:596
#, c-format
msgid ""
"%s - execute a command as another user\n"
"\n"
