#: src/parse_args.c:595
#, c-format
msgid ""
"\n"
"Options:\n"
msgstr ""
"\n"
"Опције:\n"

#: src/parse_args.c:597
msgid "use a helper program for password prompting"
msgstr "користи програм испомоћи за упит лозинке"

